{* Get generic embed block payload *}
{[ const block = context /]}

{* Create generic embed block *}
{[ if block.url ]}
  <div class="embed-url">
    <iframe class="embed-responsive-item" src="{{ block.url }}" allowfullscreen allow="autoplay" loading="lazy"></iframe>
    {[ if block.caption ]}
        <p class="caption">{{ block.caption }}</p>
    {[/]}
  </div>
{[/]}
